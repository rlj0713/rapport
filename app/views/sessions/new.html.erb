<h1>Rapport</h1>
<h4>Keep the conversation going.</h4><br>
<h2>Log-In</h2>
<%= form_tag "/sessions" do %><br>
  <label>Username</label><br>
  <%= text_field_tag :'user[username]' %><br>

  <label>Password</label><br>
  <%= password_field_tag :'user[password]' %>

  <h5><%= @error %></h5><br>


  <%= submit_tag "Log-In" %><br><br>
<% end %>

<%= link_to('Log in with Google', '/auth/google_oauth2', method: :post) %>